{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            ">= 5%",
            "last 10 versions",
            "not ie <= 8"
          ]
        }
      }
    ],
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
        // or 'css'
      }
    ],
    "transform-decorators-legacy"
  ]
}
